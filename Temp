rtlCode = rtlCode.replace(
  /\.simulate\('blur',\s*(\{.*?target:\s*\{.*?value:\s*'(.*?)'.*?\}\}\)/g,
  (match, blurData, value) => {
    console.log("Match found:", match); // For debugging
    // Return the transformed code
    return `fireEvent.blur(emailField, { target: { value: '${value}' } })`;
  }
);
