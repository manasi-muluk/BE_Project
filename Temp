// Sample file content stored in the variable `temp`
let temp = `
  emailField.simulate('blur', {target: { value: '$#%$@^$&%*' } });
  username.simulate('blur', {target: { value: 'testValue' } });
`;

// Logic to find and replace the new pattern in the `temp` variable
temp = temp.replace(
  /(\w+)\.simulate\('blur',\s*(\{.*?\})\);/g,
  'fireEvent.blur($1, $2);'
);

// Output the updated content
console.log(temp);
